{
  "name": "zonal",
  "version": "0.5.0",
  "description": "Zonal Statistics",
  "main": "zonal.js",
  "unpkg": "zonal.min.js",
  "jsdelivr": "zonal.min.js",
  "files": [
    "index.js",
    "zonal.js",
    "zonal.min.js"
  ],
  "scripts": {
    "babel": "./node_modules/.bin/babel index.js > zonal.js",
    "browserify": "npx browserify zonal.js > zonal.min.js",
    "build": "npm run babel && npm run browserify",
    "format": "npx prettier --arrow-parens=avoid --print-width=120 --trailing-comma=none --write test.js zonal.js",
    "setup": "cd data && ./setup.sh",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oviohub/zonal.git"
  },
  "keywords": [
    "analysis",
    "geospatial",
    "gis",
    "raster",
    "zonal"
  ],
  "author": "Daniel J. Dufour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oviohub/zonal/issues"
  },
  "homepage": "https://github.com/oviohub/zonal#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@turf/buffer": "^6.5.0",
    "@turf/meta": "^6.5.0",
    "flug": "^2.2.0"
  },
  "dependencies": {
    "@turf/area": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/clone": "^6.5.0",
    "@turf/difference": "^6.5.0",
    "papaparse": "^5.3.1"
  }
}
